{
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontWeightBold": "normal",
    "workbench.startupEditor": "newUntitledFile",
    "C_Cpp.updateChannel": "Insiders",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.idea": true,
        "**/.project": true,
        "**/.pytest_cache": true,
        "**/.settings": true,
        "**/node_modules": true
    },
    "terminal.integrated.macOptionClickForcesSelection": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": false,
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "files.insertFinalNewline": true,
    "cmake.configureOnOpen": true,
    "vim.camelCaseMotion.enable": true,
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.useSystemClipboard": true,
    "editor.lineNumbers": "relative",
    "vim.gdefault": true,
    "vim.leader": ",",
    "python.formatting.provider": "black",
    "go.formatTool": "gofmt",
    "go.useLanguageServer": true,
    "go.inferGopath": true,
    "window.zoomLevel": 0,
    "cSpell.userWords": [
        "cors",
        "Girogate",
        "girolink",
        "gitlab",
        "grpc",
        "kube",
        "kubernetes",
        "pkgconfig",
        "postgresql",
        "ppro",
        "println",
        "Proto",
        "protobuf",
        "pytest",
        "struct",
        "submodule",
        "substr",
        "vararg",
        "Vousden"
    ],
    "sqltools.autoOpenSessionFiles": false,
    "python.jediEnabled": false,
    "[go]": {
        "editor.useTabStops": true
    },
    "[makefile]": {
        "editor.useTabStops": true,
        "editor.insertSpaces": false,
        "editor.detectIndentation": false
    },
    "files.associations": {
        "Dockerfile.*": "dockerfile"
    },
    "extensions.ignoreRecommendations": false,
    "[kotlin]": {
        "editor.defaultFormatter": "esafirm.kotlin-formatter"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "gitlab.instanceUrl": "https://gitlab.ppro.com",
    "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.addCommentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "u"
            ],
            "commands": [
                "editor.action.removeCommentLine",
                "<esc>"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "k"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.addCommentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "u"
            ],
            "commands": [
                "editor.action.removeCommentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                "references-view.find"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
    ],
    "git.autofetch": true,
    "cSpell.enableFiletypes": [
        "kotlin"
    ],
    "workbench.preferredLightColorTheme": "Solarized Custom Light",
    "window.autoDetectColorScheme": true,
    "workbench.preferredDarkColorTheme": "Solarized Custom Dark",
    "workbench.colorTheme": "Solarized Custom Light",
    "workbench.colorCustomizations": {
        "[Solarized Custom Dark]": {
            "editorGutter.background": "#073642",
            "terminal.foreground": "#839496",
            "terminal.background": "#002b36",
            "terminal.ansiBrightBlack": "#002b36", // base03
            "terminal.ansiBlack": "#073642", // base02
            "terminal.ansiBrightGreen": "#586e75", // base01
            "terminal.ansiBrightYellow": "#657b83", // base00
            "terminal.ansiBrightBlue": "#839496", // base0
            "terminal.ansiBrightCyan": "#93a1a1", // base1
            "terminal.ansiWhite": "#eee8d5", // base2
            "terminal.ansiBrightWhite": "#fdf6e3", //base3
        },
        "[Solarized Custom Light]": {
            "editorGutter.background": "#eee8d5",
            "terminal.foreground": "#657b83",
            "terminal.background": "#fdf6e3",
            "terminal.ansiBrightWhite": "#002b36", // base03
            "terminal.ansiWhite": "#073642", // base02
            "terminal.ansiBrightCyan": "#586e75", // base01
            "terminal.ansiBrightBlue": "#657b83", // base00
            "terminal.ansiBrightYellow": "#839496", // base0
            "terminal.ansiBrightGreen": "#93a1a1", // base1
            "terminal.ansiBlack": "#eee8d5", // base2
            "terminal.ansiBrightBlack": "#fdf6e3", //base3
        }
    },
    "go.toolsManagement.autoUpdate": true,
    "aws.profile": "profile:personal",
    "aws.suppressPrompts": {
        "fileViewerEdit": true
    },
    "editor.formatOnSave": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "redhat.telemetry.enabled": false
}